{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": false,
    "skipLibCheck": true,
    "target": "esnext"
  },
  "include": ["**/*.js", "**/*.jsx", "App.js"],
  "exclude": ["**/node_modules", "**/Pods", "android/**", "ios/**"]
}
